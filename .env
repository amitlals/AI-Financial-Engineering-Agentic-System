# Environment variables for configuration

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=financial_db
DB_USER=admin
DB_PASSWORD=secret

# API configuration
API_KEY=your_api_key_here
API_SECRET=your_api_secret_here

# SAP OData connection configuration
SAP_ODATA_BASE_URL=https://your-sap-server.com/sap/opu/odata/sap/your_service
SAP_ODATA_USERNAME=your_sap_username
SAP_ODATA_PASSWORD=your_sap_password
SAP_ODATA_CLIENT=100

# Application settings
DEBUG=True
LOG_LEVEL=INFO
